<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
   <key>Label</key>
   <string>de.wycomco.misty</string>
   <key>ProgramArguments</key>
   <array>
       <string>/bin/zsh</string>
       <string>/usr/local/wycomco/misty</string>
   </array>
   <key>StartCalendarInterval</key>
   <dict>
       <key>Hour</key>
       <integer>22</integer>
       <key>Minute</key>
       <integer>0</integer>
   </dict>
   <key>StandardOutPath</key>
   <string>/var/log/misty.log</string>
   <key>StandardErrorPath</key>
   <string>/var/log/misty_error.log</string>
</dict>
</plist>

